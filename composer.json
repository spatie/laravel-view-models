{
    "name": "spatie/laravel-view-models",
    "description": "View models in Laravel",
    "keywords": [
        "spatie",
        "laravel-view-models"
    ],
    "homepage": "https://github.com/spatie/laravel-view-models",
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Roose",
            "email": "brent@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ViewModels\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ViewModels\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ViewModels\\Providers\\ViewModelsServiceProvider"
            ]
        }
    }
}
